{
  "name": "gpc-ko-widgets",
  "version": "0.0.1",
  "description": "GPC Knockout-based widget library",
  "main": "src/main.js",
  "lib": "src/",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
