div.command-panel
  // ko foreach: groups
  div.group(data-bind="style: { top: top()+'px' }, foreach: commands")
    div.command-button(data-bind="text: name, style: { display: visible() ? '' : 'none' }, css: { enabled: enabled() }")
  div.bracket(data-bind="style: { top: top()+'px', height: height()+'px' }")
  // /ko
